{
  "kind": "collectionType",
  "collectionName": "rewrite_categories",
  "info": {
    "singularName": "rewrite-category",
    "pluralName": "rewrite-categories",
    "displayName": "RewriteCategory"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "emoji": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "is_banner": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": false
    },
    "templates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rewrite-template.rewrite-template"
    }
  }
}
